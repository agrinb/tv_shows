<h3><%= @television_show.title %></h3>

<ul>
  <li>Network: <%= @television_show.network %></li>
  <li>Years: <%= @television_show.years %></li>
</ul>

<div>
  <%= @television_show.synopsis %>
</div>

<div>
  <%= render 'characters/new' %>
</div>


<div>
  <ul>
  <% @television_show.characters.each do |character| %>
    <li><%= "Character: #{character.character} - Actor: #{character.actor.name} "%><%= link_to "Delete", television_show_character_path(@television_show, character), method: :delete %>


</li>
    <% end %>
  </ul>
</div>


